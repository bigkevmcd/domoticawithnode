d3.csv("./power.csv")
    .row(function(d) { return {
        period: new Date(d.Timestamp), total: +d.Current
      }
    })
    .get(function(error, data) { 
      console.log(data);
      var dataSources = [
        {
          id: 'power-timeline',
          title: 'kilowatt/hour',
          dimensions: {
            x: 'time',
            y: 'kw/h',
          },
          data: data
        }
      ];
      var timelineGraph = glimpse.graphBuilder.create('line').data(dataSources).config({yAxisUnit: ' kw/h'}).render('#timeline');
      // console.log(timelineGraph.config());
  });

